<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mvp.firstlapmaterialdesgin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".screens.screens.framescreen.FrameActivity"></activity>
        <activity android:name=".screens.screens.relativescreen.RelativeActivity" />
        <activity android:name=".screens.screens.linearscreen.LinearScreen" />
        <activity android:name=".screens.screens.gridscreen.GridActivity" />
        <activity android:name=".screens.screens.constraintscreen.ConstraintActivity" />
        <activity android:name=".screens.screens.mainscreen.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>